+            final int[] systemUnlockedUsers;
+            synchronized (mLock) {
+                // make copy as sorting can change order
+                systemUnlockedUsers = Arrays.copyOf(mSystemUnlockedUsers,
+                        mSystemUnlockedUsers.length);
